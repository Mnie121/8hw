import unittest

def dele(a, b):
    return a/b

class divTest(unittest.TestCase):
    def testDelePol(self):
        self.assertEqual(dele(10, 5), 2)
    def testDeleNeg(self):
        self.assertEqual(dele(-10, 5), -2)
    def testDeleZero(self):
        self.assertEqual(dele(0, 5), 0)
        
if __name__ == "__main__":
    unittest.main()
